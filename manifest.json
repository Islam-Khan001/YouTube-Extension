{
    "manifest_version": 3,
    "name": "YouTube Controls (For Students)",
    "version": "0.1.1",
    "description": "A Chrome extension that gives you more control over YouTube.",
    "author": "Islam kathat",
    "homepage_url": "https://github.com/Islam-Khan001",
    "icons": {
      "16": "myicon16.png",
      "48": "myicon48.png",
      "128": "myicon128.png"
  },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "myicon16.png",
        "48": "myicon48.png",
        "128": "myicon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches": ["*://www.youtube.com/*"],
        "js": ["content.js"],
        "css": ["contentStyle.css"]
      }
    ]
  }
  